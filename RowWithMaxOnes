// Given a boolean 2D array, where each row is sorted. Find the row with the maximum number of 1s.

public static int RowWithMaxOnes(ArrayList<ArrayList<Integer>> A){
    int ans = 0;
    int i = 0;
    int N = A.size();
    int M = A.get(0).size();
    int j = M - 1;
    while (i < N){
        while ( j >= 0 && A.get(i).get(j) == 1){
            j--;
            ans = i;
        }
        i++;
    }
    return ans;
}
